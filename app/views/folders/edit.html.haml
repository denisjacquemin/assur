.block
  .content
    %h2.title
      = "#{t("assur.general.Details")} #{t("assur.folder.folder")}: "
      = @folder.name
    .inner
      .inner
        = form_for @folder, :url => folders_path, :html => { :class => :form } do |f|
          = render :partial => "form", :locals => {:f => f}
- content_for :sidebar, render(:partial => 'edit_sidebar')